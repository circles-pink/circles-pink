{
  "purescript.outputDirectory": "pkgs/ts/@circles-pink/state-machine/output/",
  "purescript.buildCommand": "just spago-build-ide",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "[purescript]": {
    "editor.defaultFormatter": "nwolverson.ide-purescript"
  },
  "purescript.exportsCodeLens": true,
  "[dhall]": {
    "editor.defaultFormatter": "dhall.vscode-dhall-lsp-server"
  },
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "purescript.formatter": "purs-tidy"
}
